trolDirective#pristine\n - api/forms/AbstractControlDirective#reset\n - api/forms/AbstractControlDirective#status\n - api/forms/AbstractControlDirective#statusChanges\n - api/forms/AbstractControlDirective#touched\n - api/forms/AbstractControlDirective#untouched\n - api/forms/AbstractControlDirective#valid\n - api/forms/AbstractControlDirective#value\n - api/forms/AbstractControlDirective#valueChanges\n - api/forms/AsyncValidatorFn\n - api/forms/ControlContainer\n - api/forms/ControlContainer#formDirective\n - api/forms/ControlContainer#name\n - api/forms/ControlContainer#path\n - api/forms/FormArray\n - api/forms/FormArray#push\n - api/forms/FormArrayName#asyncValidator\n - api/forms/FormArrayName#control\n - api/forms/FormArrayName#description\n - api/forms/FormArrayName#example\n - api/forms/FormArrayName#formDirective\n - api/forms/FormArrayName#formarrayname\n - api/forms/FormArrayName#name\n - api/forms/FormArrayName#ngOnDestroy\n - api/forms/FormArrayName#ngOnInit\n - api/forms/FormArrayName#ngondestroy\n - api/forms/FormArrayName#ngoninit\n - api/forms/FormArrayName#path\n - api/forms/FormArrayName#selectors\n - api/forms/FormArrayName#validator\n - api/forms/FormArrayName#属性\n - api/forms/FormArrayName#方法\n - api/forms/FormArrayName#输入参数\n - api/forms/FormControl\n - api/forms/FormControlName\n - api/forms/FormGroup\n - api/forms/FormGroupDirective\n - api/forms/ReactiveFormsModule\n - api/forms/ValidatorFn\n - https://github.com/angular/angular/edit/master/packages/forms/src/directives/reactive_directives/form_group_name.ts?message=docs(forms)%3A%20describe%20your%20change...#L168-L172\n - https://github.com/angular/angular/edit/master/packages/forms/src/directives/reactive_directives/form_group_name.ts?message=docs(forms)%3A%20describe%20your%20change...#L173-L178\n - https://github.com/angular/angular/edit/master/packages/forms/src/directives/reactive_directives/form_group_name.ts?message=docs(forms)%3A%20describe%20your%20change...#L97-L199\n - https://